# int array

var a = [12, 3, 4]
assert(a.size() == 3)
assert(a[2] == 4)
a.add(10)
assert(a[3] == 10)
assert(a.size() == 4)
assert(a[1] == 3)
a.insert(1, 8)
assert(a[1] == 8)
a[0] += 1000
assert(a[0] == 1012)

# float array
var a2 = [12.2, 3.14, 0.4, 2]
assert(a2.size() == 4)
assert(a2[2] == 0.4)
a2.add(10.89)
assert(a2[4] == 10.89)
assert(a2.size() == 5)
assert(a2[1] == 3.14)
a2.insert(1, -8.34)
assert(a2[1] == -8.34)
a2[0] *= 10
assert(a2[0] == 122.0)

# boolean array
var a3 = [true]
assert(a3.size() == 1)
assert(a3[0])
a3.add(false)
assert(!a3[1])
assert(a3.size() == 2)
a3.insert(1, true)
assert(a3[1])
a3[0] = false
assert(!a3[0])

# object array
var a4 = ["hello", "world", "!!"]
assert(a4.size() == 3)
assert(a4[2] == "!!")
a4.add("??")
assert(a4[3] == "??")
assert(a4.size() == 4)
assert(a4[1] == "world")
a4.insert(1, "shell")
assert(a4[1] == "shell")
a4[0] += " world"
assert(a4[0] == "hello world")

