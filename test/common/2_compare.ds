// int value
var a = 4
var b = 2
var c = 2

assert(!(a == c))
assert(a != b)
assert(b < a)
assert(a > b)
assert(!(a <= b))
assert(b >= c)

// float value
var a2 = 4.2
var b2 = 2.2
var c2 = 2.2

assert(!(a2 == c2))
assert(a2 != b2)
assert(b2 < a2)
assert(a2 > b2)
assert(!(a2 <= b2))
assert(b2 >= c2)

// int and float value
var a3 = 4.2
var b3 = 2
var c3 = 2.0

assert(!(a3 == c3))
assert(a3 != b3)
assert(b3 < a3)
assert(a3 > b3)
assert(!(a3 <= b3))
assert(b3 == c3)
